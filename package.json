{
  "name": "realtime-chat",
  "version": "1.0.0",
  "description": "베지아보카도 리얼타임 채팅 ",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha tests/*.js",
    "test:coverage": "open coverage/lcov-report/index.html",
    "lint:tests": "eslint ./tests/js --ext .js --fix",
    "lint:src": "eslint ./src --ext .js --fix",
    "start": "node server.js",
    "serve": "nodemon ./server.js --exec babel-node --presets env,stage-0"
  },
  "author": "peepee + willy",
  "license": "MIT",
  "repository": "https://github.com/veggieavocado/realtime-chat.git",
  "devDependencies": {
    "yarn": "^1.7.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "chai-immutable": "^1.6.0",
    "commander": "^2.16.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "immutable": "^3.8.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mongodb": "^3.1.1",
    "mongoose": "^5.2.4",
    "nodemon": "^1.18.3",
    "raven": "^2.6.3",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
